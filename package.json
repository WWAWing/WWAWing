{
  "name": "@wwawing/wwawing",
  "version": "3.1.6",
  "description": "World Wide Adventure: an RPG Engine.",
  "main": "wwa.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:js && npm run build:dist",
    "build:js": "npm run build:js:webpack && npm run build:js:cat",
    "build:js:webpack": "webpack --config webpack.config.js && webpack --config webpack.config.prod.js",
    "build:js:cat": "shx cat assets/wwa_license_comment.js ./lib/wwa-nolink.js assets/cryptojs/aes.js > ./lib/wwa.js",
    "build:dist": "npm run clean:dist && node ./util/make-wwawing-dist && node ./util/make-zip",
    "deploy": "shx cp -R assets debug && shx cp lib/wwa.long.js debug",
    "clean": "npm run clean:dist && npm run clean:js",
    "clean:js": "shx rm -rf lib",
    "clean:dist": "shx rm -rf wwawing-dist wwawing-update wwawing-dist.zip wwawing-update.zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WWAWing/WWAWing.git"
  },
  "keywords": [
    "wwa",
    "typescript"
  ],
  "author": "rmn_e",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WWAWing/WWAWing/issues"
  },
  "homepage": "https://github.com/WWAWing/WWAWing#readme",
  "devDependencies": {
    "shelljs": "^0.8.2",
    "shx": "^0.3.1",
    "ts-loader": "^4.3.0",
    "typescript": "^2.5.3",
    "uglify-js": "^3.3.27",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4"
  }
}
