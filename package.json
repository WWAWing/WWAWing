{
  "name": "@wwawing/wwawing",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "turbo run clean",
    "start": "turbo run start --stream --filter=@wwawing/engine",
    "build": "turbo run build --stream",
    "deploy": "turbo run deploy:prod --filter=@wwawing/engine",
    "generate": "turbo run generate --stream",
    "make-dist": "npm run build && turbo run make-dist --stream",
    "version-up": "lerna version --no-git-tag-version",
    "publish:all": "run-s build generate && lerna publish from-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WWAWing/WWAWing.git"
  },
  "keywords": [
    "WWA"
  ],
  "author": "WWA Wing Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WWAWing/WWAWing/issues"
  },
  "homepage": "https://github.com/WWAWing/WWAWing#readme",
  "devDependencies": {
    "@lerna-lite/cli": "^1.0.5",
    "@types/node": "^12.12.6",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.4.0",
    "turbo": "^1.2.5",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
