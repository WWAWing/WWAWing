{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "clean": {},
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.scss"],
      "outputs": ["lib/**"]
    },
    "deploy:prod": {
      "dependsOn": ["build"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "generate": {
      "dependsOn": ["build", "^generate"],
      "inputs": ["lib/**"],
      "outputs": [
        "bin/**"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}
