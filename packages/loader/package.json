{
  "name": "@wwawing/loader",
  "version": "3.5.8",
  "description": "a World Wide Adventure mapdata loader for client-side web applications",
  "main": "./lib/index.js",
  "module": "./module/index.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": [
    "./**/*.html",
    "./**/*.dat"
  ],
  "files": [
    "lib",
    "module"
  ],
  "scripts": {
    "start": "run-s debug",
    "test": "echo \"no test specified\"",
    "debug": "run-s debug:clean debug:build debug:server",
    "debug:clean": "run-s clean:debug",
    "debug:build": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack && shx cp ./src/___debug-resources___/wwamap.dat ./debug",
    "debug:server": "http-server ./debug -o",
    "build": "run-p build:*",
    "build:lib": "npm run clean:lib && tsc -p .",
    "build:module": "npm run clean:module && tsc -p ./tsconfig.module.json",
    "clean": "run-p clean:*",
    "clean:lib": "shx rm -rf lib",
    "clean:module": "shx rm -rf module",
    "clean:debug": "shx rm -rf debug",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WWAWing/WWALoader.git"
  },
  "author": "WWA Wing Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WWAWing/WWALoader/issues"
  },
  "homepage": "https://github.com/WWAWing/WWALoader#readme",
  "dependencies": {
    "@wwawing/event-emitter": "^3.5.8"
  },
  "devDependencies": {
    "@wwawing/common-interface": "^3.5.8",
    "cross-env": "^7.0.3",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "ts-loader": "^4.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  }
}
