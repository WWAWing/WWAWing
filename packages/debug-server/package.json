{
  "name": "@wwawing/debug-server",
  "version": "3.7.3",
  "description": "wwawing debug server",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "start": "npm run build && node lib debug",
    "build": "tsc -p .",
    "generate": "shx mkdir -p bin && pkg -t node16.13.2-win-x64 ./lib/index.js --output bin/wwa-server.exe",
    "clean": "shx rm -rf debug lib bin",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "WWA Wing Team",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/node": "^12.12.6",
    "express": "^4.17.1",
    "pkg": "^5.5.1",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "typescript": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
