{
  "name": "@wwawing/debug-server",
  "version": "0.0.0",
  "description": "wwawing debug server",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm-run-all build:*",
    "build:scripts": "tsc -p .",
    "build:exe": "shx mkdir -p bin && nexe --target=win32-x86-8.6.0 --output bin/wwa-server.exe",
    "start": "npm run build && npm run deploy && node lib debug",
    "clean": "shx rm -rf debug lib bin"
  },
  "author": "WWA Wing Team",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "express": "^4.16.3",
    "nexe": "^2.0.0-rc.31",
    "npm-run-all": "^4.1.3",
    "shelljs": "^0.8.2",
    "shx": "^0.3.1",
    "typescript": "^2.9.2"
  }
}
