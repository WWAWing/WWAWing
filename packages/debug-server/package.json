{
  "name": "@wwawing/debug-server",
  "version": "3.13.0-unstable.based-on.3.12.10.p.3",
  "description": "wwawing debug server",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "start": "npm run build && node lib debug",
    "build": "tsc -p .",
    "generate": "shx mkdir -p bin && pkg -t node18-win-x64 ./lib/index.js --output bin/wwa-server.exe",
    "clean": "shx rm -rf debug lib bin",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "WWA Wing Team",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/node": "^18.11.9",
    "@types/serve-index": "1.9.1",
    "express": "^4.18.1",
    "pkg": "^5.8.0",
    "serve-index": "1.9.1",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  }
}
