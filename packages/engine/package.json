{
  "name": "@wwawing/engine",
  "version": "3.1.6",
  "description": "World Wide Adventure: an RPG Engine.",
  "main": "wwa.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all build deploy && node ./launch-server debug",
    "build": "npm-run-all clean build:*",
    "build:webpack": "webpack --config webpack.config.js && webpack --config webpack.config.prod.js",
    "build:cat": "shx cat ./node_modules/@wwawing/assets/scripts/wwa_license_comment.js ./lib/wwa-nolink.js ./node_modules/@wwawing/assets/scripts/aes.js > ./lib/wwa.js",
    "deploy": "shx mkdir -p debug && npm-run-all deploy:*",
    "deploy:audio": "shx cp -R ./node_modules/@wwawing/assets/audio debug/",
    "deploy:html": "shx cp ./node_modules/@wwawing/assets/html/dev/*.* debug/",
    "deploy:images": "shx cp ./node_modules/@wwawing/assets/images/*.* debug/",
    "deploy:mapdata": "shx cp ./node_modules/@wwawing/assets/mapdata/*.* debug/",
    "deploy:scripts": "shx cp ./lib/wwa.long.js debug/ && shx cp ../assets/scripts/*.* debug/ && shx cp ./node_modules/@wwawing/loader/lib/wwaload*.js debug/",
    "deploy:style": "shx cp ./node_modules/@wwawing/assets/style/*.* debug/",
    "clean": "shx rm -rf lib debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WWAWing/WWAWing.git"
  },
  "keywords": [
    "wwa",
    "typescript"
  ],
  "author": "WWA Wing Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WWAWing/WWAWing/issues"
  },
  "homepage": "https://github.com/WWAWing/WWAWing#readme",
  "devDependencies": {
    "shelljs": "^0.8.2",
    "shx": "^0.3.1",
    "ts-loader": "^4.3.0",
    "typescript": "^2.5.3",
    "uglify-js": "^3.3.27",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "npm-run-all": "^4.1.3",
    "@wwawing/loader": "0.0.0",
    "@wwawing/assets": "0.0.0",
    "@wwawing/debug-server": "0.0.0"
  }
}
