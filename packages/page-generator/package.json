{
  "name": "@wwawing/page-generator",
  "version": "4.1.0-unstable.based-on.3.12.18.p.5",
  "description": "a HTML file generator for WWA Wing",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && shx mkdir -p output && node debug/debug.js",
    "build": "run-p build:*",
    "build:lib": "tsc && shx cp ./src/wwa.pug ./lib/wwa.pug",
    "build:module": "tsc -p ./tsconfig.module.json && shx cp ./src/wwa.pug ./module/wwa.pug",
    "clean": "run-s clean:*",
    "clean:lib": "shx rm -rf ./lib",
    "clean:module": "shx rm -rf ./module"
  },
  "files": [
    "lib",
    "module",
    "template",
    "config"
  ],
  "keywords": [
    "WWA"
  ],
  "author": "WWA Wing Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/pug": "^2.0.6",
    "npm-run-all2": "^8.0.1",
    "shelljs": "^0.9.2",
    "shx": "^0.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "pug": "^3.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  }
}
